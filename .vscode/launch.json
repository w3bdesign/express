  "version": "0.2.0",
  "configurations": [{
      "type": "node",
      "request": "launch",
      "name": "Node Express Debug",
      "runtimeExecutable": "node",
      "runtimeArgs": [
        "--inspect",
        "./bin/www",
        "npm",
        "start"
      ],
      "port": 9229,
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to 9229",
      "port": 9229,
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}